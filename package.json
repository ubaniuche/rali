{
  "name": "Rali",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist && babel src --out-dir dist  --copy-files",
    "start": "npm run build && node dist/index.js",
    "start:dev": "nodemon --exec babel-node src/index.js",
    "test": "npm run db-prep-test && mocha --require @babel/polyfill --require @babel/register  './src/api/v1/tests/integration/*.js'",
    "migrations": "npx sequelize db:migrate",
    "seed": "npx sequelize db:seed:all",
    "seed-test": "npx sequelize db:seed:all --env test",
    "undo-migrations": "npx sequelize db:migrate:undo:all",
    "migrations-test": "npx sequelize db:migrate --env test",
    "undo-migrations-test": "npx sequelize db:migrate:undo:all --env test",
    "db-prep": "npm run migrations && npm run seed",
    "db-prep-test": "npm run undo-migrations-test && npm run migrations-test && npm run seed-test"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "app-root-path": "^3.0.0",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-fileupload": "^1.3.1",
    "express-validator": "^6.14.0",
    "mocha": "^9.2.2",
    "moment": "^2.29.3",
    "node-cron": "^3.0.0",
    "pg": "^8.7.3",
    "sequelize": "^6.19.0",
    "sequelize-cli": "^6.4.1",
    "winston": "^3.7.2",
    "winston-daily-rotate-file": "^4.6.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.9",
    "@babel/node": "^7.16.8",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.16.11",
    "@babel/register": "^7.17.7",
    "@babel/runtime": "^7.17.9",
    "nodemon": "^2.0.15",
    "rimraf": "^3.0.2"
  }
}
